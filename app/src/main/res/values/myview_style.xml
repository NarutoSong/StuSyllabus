<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="style_main_table_tv">
        <item name="android:layout_width">@dimen/main_curriculum_textview_width</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:lines">4</item>
        <item name="android:paddingBottom">@dimen/main_table_row0_textpadding</item>
        <item name="android:paddingTop">@dimen/main_table_row0_textpadding</item>
        <item name="android:textSize">@dimen/main_curriculum_textsize</item>
        <item name="android:background">@color/white</item>
    </style>

    <style name="style_main_table_tv_headerleft">
        <item name="android:layout_width">20dp</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="style_main_table_tv_headertop">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:paddingBottom">@dimen/main_table_row0_textpadding</item>
        <item name="android:paddingTop">@dimen/main_table_row0_textpadding</item>
    </style>

    <style name="style_main_table_tablerow">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_marginLeft">@dimen/main_margin_horizontal</item>
        <item name="android:layout_marginRight">@dimen/main_margin_horizontal</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="stylle_main_table_headerleft_linearlayout">
        <item name="android:layout_width">15dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="style_login_et">
        <item name="android:layout_width">216dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_marginTop">60dp</item>
        <item name="android:background">@drawable/login_et_shape</item>
        <item name="android:inputType">number</item>
        <item name="android:textColor">#51636D</item>
        <item name="android:textSize">12dp</item>
    </style>
</resources>